" vim settings for gui mode

" Font Settings
  set guifont=SauceCodePro\ Nerd\ Font\ Mono\ 10,SauceCodePro\ NF,Source\ Code\ Variable\ 10

" Default window size
  set lines=50 columns=150

" Disable scrollbars
  set guioptions-=r
  set guioptions-=R
  set guioptions-=l
  set guioptions-=L
  set guioptions-=b

